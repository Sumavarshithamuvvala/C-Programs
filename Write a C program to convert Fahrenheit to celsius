#include <stdio.h>

int main()
{
    float fahrenheit, celsius;
    printf("Enter temperature in Fahrenheit: ");
    scanf("%f", &fahrenheit);
    celsius = (fahrenheit - 32) * 5 / 9;
    printf("Temperature in Celsius: %f\n", celsius);
    
}
Output
Enter temperature in Fahrenheit: 273
Temperature in Celsius: 133.888885
